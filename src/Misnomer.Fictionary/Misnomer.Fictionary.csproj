<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net461;netcoreapp1.0;netstandard1.2;netstandard2.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <NeutralLanguage>en-US</NeutralLanguage>
    <AssemblyVersion>0.1</AssemblyVersion>
    <VersionPrefix>0.1.0</VersionPrefix>
    <FileVersion>$(VersionPrefix)</FileVersion>
    <Authors>Viktor T.</Authors>
    <Title>Fast dictionary</Title>
    <Description>Associative collection like usual Dictionary&lt;K, V&gt;, but with comparer type preserved instead of hiding behind the interface.
Enables devirtualization of calls to the key comparer.
Enables reusing instances of inner array of entries via ArrayPool&lt;T&gt;.Shared.
Provides ToFictionary() extension methods which perfectly replaces conventional ToDictionary() for materializing LINQ queries.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/qbit86/misnomer</RepositoryUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <PackageTags>fast;dictionary;generic;collection;pool;memory;allocation;jit</PackageTags>
    <PackageIconUrl>https://github.com/qbit86/misnomer/raw/master/kopimi.png</PackageIconUrl>
    <PackageReleaseNotes>https://github.com/qbit86/misnomer/blob/develop/src/Misnomer.Fictionary/CHANGELOG.md</PackageReleaseNotes>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(MisnomerRootDir)misnomer.snk</AssemblyOriginatorKeyFile>
    <RootNamespace>Misnomer</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition="'$(DebugType)' == 'portable'">
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CodeQuality.Analyzers" Version="2.6.3" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0-beta2-18618-05">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>      
    <PackageReference Condition="'$(TargetFramework)' != 'netcoreapp1.0'" Include="System.Buffers" Version="4.5.0" />
    <PackageReference Include="Text.Analyzers" Version="2.6.3" />
  </ItemGroup>
  <Import Project="..\Misnomer.Internal\Misnomer.Internal.projitems" Label="Shared" />
</Project>
