<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard1.1</TargetFramework>
    <LangVersion>latest</LangVersion>
    <NeutralLanguage>en-US</NeutralLanguage>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
    <Version>0.1.0</Version>
    <Authors>Viktor T.</Authors>
    <Title>Recyclable list</Title>
    <Description>Indexed collection like usual List&lt;T&gt;, but with less allocations.
Enables reusing instances of inner array via ArrayPool&lt;T&gt;.Shared.
Provides ToRist() extension method which perfectly replaces conventional ToList() call materializing LINQ queries.</Description>
    <PackageLicenseUrl>https://github.com/qbit86/misnomer/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/qbit86/misnomer</RepositoryUrl>
    <PackageTags>recyclable;collection;pool;memory;allocation;list;array;buffer</PackageTags>
    <PackageIconUrl>https://github.com/qbit86/misnomer/raw/develop/kopimi.png</PackageIconUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.Buffers" Version="4.4.0" />
  </ItemGroup>
  <Import Project="..\Misnomer.Internal\Misnomer.Internal.projitems" Label="Shared" />
</Project>